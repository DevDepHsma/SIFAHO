
<%= form_with url: get_by_enrollment_and_fullname_professionals_path, remote: true, method: :GET, id: "professional-form-async" do |form| %>
  <div class="form-group">
    <input class="form-control" id="last-name" name="last_name" type="text" placeholder="Apellido" autocomplete="off" value="<%= @professional.last_name if @professional.present? %>" />
  </div>
  <div class="form-group">
    <input class="form-control" id="first-name" name="first_name" type="text" placeholder="Nombre" autocomplete="off" value="<%= @professional.first_name if @professional.present? %>" />
  </div>
  <input type="hidden" name="id" value="<%= @professional.id if @professional.present? %>">
<% end %>

<div id="professionals-list"></div>