$("tr#child-<%= @order_product.id.to_s %>").find('button.btn-select-lot-stock').first().text('<%= @order_product.order_prod_lot_stocks.count.to_s %>');

$('#lot-selection').modal('hide');

setLotSelectionProgress($("tr#child-<%= @order_product.id.to_s %>"), parseInt("<%= @order_product.order_prod_lot_stocks.sum(:reserved_quantity) %>"), parseInt("<%= @order_product.parent.total_dose %>"));