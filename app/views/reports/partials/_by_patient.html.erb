<thead>
  <tr>
    <th>CÃ³d producto</th>
    <th>Producto</th>
    <th>Paciente</th>
    <th class="text-center">Dni</th>
    <th class="text-center">Edad</th>
    <th class="text-center">Fecha nacimiento</th>
    <th>Cantidad</th>
