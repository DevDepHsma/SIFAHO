<%= render 'establishments/external_orders/header' %>
<% if @establishments.present? %>
  <div class="card fixed-custom-card">
    <div class="card-header">
      <%= render 'filter' %>
    </div>
    <div class="card-body fixed-panel-index">
      <% if @establishments.present? %>
        <div id="filterrific_results">
          <%= render(
            partial: 'list',
            locals: { establishments: @establishments }
          ) %>
        </div>
      <% else %>
        <%= render(
          partial: 'shared/not_found_filter',
          locals: { object_name: 'establecimientos' }
        ) %>
      <% end %>
    </div>
  </div>
<% else %>
  <%= render(
    partial: 'shared/not_found',
    locals: { object_name: 'establecimientos' }
  ) %>
<% end %>
<% content_for :modal do %>
  <div id="modals">
    <%= render(partial: 'shared/modals/delete_confirmation') %>
  </div>
<% end %>
