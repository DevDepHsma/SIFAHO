<%= render 'sector_supply_lots/header' %>

<% if @receipts.present? %>

    <div class="card fixed-custom-card">
      <div class="card-header">
        <%= render 'filter'%>
      </div>
      <div class="card-body fixed-panel-index">
        <%= render(
          partial: 'receipts/list',
          locals: { receipts: @receipts }
        ) %>
      </div>
    </div>

<% else %>
  <%= render(
    partial: 'shared/not_found',
    locals: { object_name: 'recibos' }
  ) %>
<% end %>
