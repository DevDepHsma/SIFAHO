<p id="notice"><%= notice %></p>

<h1>Receipts</h1>

<table>
  <thead>
    <tr>
      <th>Supply</th>
      <th>Supply lot</th>
      <th>Lot code</th>
      <th>Laboratory name</th>
      <th>Expiry date</th>
      <th>Quantity</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receipts.each do |receipt| %>
      <tr>
        <td><%= receipt.supply %></td>
        <td><%= receipt.supply_lot %></td>
        <td><%= receipt.lot_code %></td>
        <td><%= receipt.laboratory_name %></td>
        <td><%= receipt.expiry_date %></td>
        <td><%= receipt.quantity %></td>
        <td><%= receipt.code %></td>
        <td><%= link_to 'Show', receipt %></td>
        <td><%= link_to 'Edit', edit_receipt_path(receipt) %></td>
        <td><%= link_to 'Destroy', receipt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receipt', new_receipt_path %>
