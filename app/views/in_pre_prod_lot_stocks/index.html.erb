<p id="notice"><%= notice %></p>

<h1>In Pre Prod Lot Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Inpatient prescription product</th>
      <th>Lot stock</th>
      <th>Dispensed by</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_pre_prod_lot_stocks.each do |in_pre_prod_lot_stock| %>
      <tr>
        <td><%= in_pre_prod_lot_stock.inpatient_prescription_product %></td>
        <td><%= in_pre_prod_lot_stock.lot_stock %></td>
        <td><%= in_pre_prod_lot_stock.dispensed_by %></td>
        <td><%= in_pre_prod_lot_stock.quantity %></td>
        <td><%= link_to 'Show', in_pre_prod_lot_stock %></td>
        <td><%= link_to 'Edit', edit_in_pre_prod_lot_stock_path(in_pre_prod_lot_stock) %></td>
        <td><%= link_to 'Destroy', in_pre_prod_lot_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New In Pre Prod Lot Stock', new_in_pre_prod_lot_stock_path %>
